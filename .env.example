# CoralSend Environment Variables
# Copy this file to .env and fill in your values

# Base Path for subdirectory deployment
# If deploying to root (/), leave empty or don't set
# If deploying to /coralsend, set to: /coralsend
# Must start with / if set
NEXT_PUBLIC_BASE_PATH=/coralsend

# WebSocket Signaling Server URL
# For production with subdirectory: wss://yourdomain.com/ws
# For local development: ws://localhost:8080/ws
# If not set, will be auto-detected from current URL
NEXT_PUBLIC_SIGNALING_URL=wss://example.com/ws

